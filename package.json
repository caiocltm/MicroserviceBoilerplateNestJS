    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commander": "^7.0.0",
    "helmet": "^4.6.0",
    "inquirer": "^7.3.3",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.1",
    "ora": "^5.4.1",
